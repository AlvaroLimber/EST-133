#Para seleccionar 8 hogares
tabla_aleatoria8 <- function(min, f, k){
nf <- (f - min)+1
m <- min
x <- matrix(0, nrow = nf*2, ncol = k)
set.seed(1)
for (i in 1:nrow(x)) {
if (i %% 2 == 0) {
x[i,]<- sample(1:k, k, replace=F)
}
else{
x[i,] <- sample(1:m,k,replace = F)
x[i,] <- sort(x[i,])
m<-m+1
}
}
y<-matrix(c("Predio", "Bloque"), byrow=T, nrow=nrow(x))
x<-cbind(y,x)
colnames(x) <- c("Clase","P1","P2","P3","P4","P5","P6","P7","P8")
nombres <-c("Menores e iguales a 8",9:f)
rownames(x) <- rep(nombres, each =2)
return(x )
}
tabla_aleatoria8(8, 50, 8)
tabla_aleatoriax<-tabla_leatoria8(8,50,8)
write.csv(x = tabla_aleatoriax, file = "C:/Users/Hp/Desktop/aru2020/Viñetas/Cuestionario/tabla_aleatoria8.csv", row.names =T)
